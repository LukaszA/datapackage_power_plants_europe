  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "wgs84 = pyproj.Proj(\"+init=EPSG:4326\")  # LatLon with WGS84 datum used by GPS units and Google Earth\n",
    "ch1903 = pyproj.Proj(\"+init=EPSG:21781\")  # CH1903 projection used in BFE-Data\n",
    "\n",
    "data_CH[['Lon', 'Lat']] = data_CH[\n",
    "    ['ZE-Koordinaten unscharf (Nord)', 'ZE-Koordinaten unscharf (Ost)']].apply(\n",
    "        lambda row: pyproj.transform(\n",
    "            ch1903, wgs84, row[1], row[0]), axis=1).apply(pd.Series)\n"
   ]
